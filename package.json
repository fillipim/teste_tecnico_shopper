{
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/app.ts",
